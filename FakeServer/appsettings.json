{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Error"
    }
  },
  "Serilog": {
    "WriteTo": [
      { "Name": "File" }
    ]
  },
  "DataStore": {
    "IdField": "id",
    "EagerDataReload": true
  },
  "Api": {
    "UpsertOnPut": false,
    "UseResultObject": false
  },
  "Caching": {
    "ETag": {
      "Enabled": true
    }
  },
  "Jobs": {
    "DelayMs": 10000
  },
  "Simulate": {
    "Delay": {
      "Enabled": false,
      "Methods": [ "GET", "POST", "PUT", "PATCH", "DELETE" ],
      "MinMs": 500,
      "MaxMs": 5000
    },
    "Error": {
      "Enabled": false,
      "Methods": [ "POST", "PUT", "PATCH", "DELETE" ],
      "Probability": 50
    }
  }
}